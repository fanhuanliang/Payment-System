{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
      "airbnb",
      "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types":[0],
        "react/destructuring-assignment":[0],
        "no-alert": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off",
        "no-lonely-if": "off",
        "no-shadow": "off",
        "consistent-return": "off",
        "react/jsx-filename-extension": [
          1,
          {
            "extensions": [
              ".js",
              ".jsx"
            ]
          }
        ],
        "import/extensions": "off"
          // "no-unused-vars": "warn",
          // "func-names": "off",
          // "no-process-exit": "off",
          // "object-shorthand": "off",
          // "class-methods-use-this": "off",
    }
}
